    {
      "mysqlOptions": {
        "authProtocol": "default"
      },
      "previewLimit": 50,
      "server": "localhost",
      "port": 3306,
      "driver": "MySQL",
      "name": "posteverydayDB",
      "database": "posteveryday",
      "username": "root",
      "password": "9001"
    }
  ]
}